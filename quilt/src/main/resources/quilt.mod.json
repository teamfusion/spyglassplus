{
  "schema_version": 1,
  "mixin": [
    "spyglassplus.mixins.json",
    "spyglassplus-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "spyglassplus",
    "version": "${version}",
    "name": "Spyglass+",
    "description": "A Minecraft mod that adds to and improves the Spyglass experience!",
    "authors": [
      "Team Fusion"
    ],
    "contact": {
      "sources": "https://github.com/teamfusion/spyglassplus",
      "issues": "https://github.com/teamfusion/spyglassplus/issues"
    },
    "license": [
      "MIT (Code)",
      "All Rights Reserved (Assets)"
    ],
    "icon": "assets/spyglassplus/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "com.github.teamfusion.spyglassplus.quilt.SpyglassPlusQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19"
      },
      {
        "id": "architectury",
        "version": ">=6.2"
      },
      {
        "id": "cloth-config",
        "version": "*"
      }
    ]
  }
}
